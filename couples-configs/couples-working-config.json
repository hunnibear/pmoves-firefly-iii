{
  "date": "Y-m-d",
  "default_account": 10,
  "delimiter": "comma",
  "headers": true,
  "rules": true,
  "skip_form": false,
  "add_import_tag": true,
  "specifics": [
    "AppendHash"
  ],
  "roles": [
    "date_transaction",
    "description",
    "amount",
    "note",
    "account-name",
    "opposing-name"
  ],
  "do_mapping": {
    "0": false,
    "1": false,
    "2": false,
    "3": false,
    "4": true,
    "5": true
  },
  "mapping": {
    "4": {
      "Joint Checking": 10
    },
    "5": {
      "Grocery Store": 12,
      "Restaurant": 14,
      "Utilities Company": 15,
      "Salary Income": 16,
      "Gas Station": 13
    }
  },
  "duplicate_detection_method": "classic",
  "ignore_duplicate_lines": true,
  "ignore_duplicate_transactions": true,
  "version": 3,
  "flow": "csv"
}